generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model problems {
  id                 Int          @unique @default(autoincrement())
  name               String
  description        String
  input_description  String
  output_description String
  mb_memory_limit    Int
  seconds_limit      Int
  tests              Bytes?
}